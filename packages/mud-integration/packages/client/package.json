{
  "name": "gribi-mud",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && npm run generate:types",
    "generate:types": "tsc -p tsconfig-build.json --declaration --emitDeclarationOnly true --outdir ./dist",
    "preview": "vite preview"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/gribi-mud.umd.js",
  "module": "./dist/gribi-mud.es.js",
  "exports": {
    ".": {
      "import": "./dist/gribi-mud.es.js",
      "require": "./dist/gribi-mud.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vite-plugin-dts": "^3.7.3"
  },
  "dependencies": {
    "mud": "^1.41.2",
    "viem": "^2.7.19"
  }
}
